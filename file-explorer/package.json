{
  "name": "file-explorer",
  "version": "0.5.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href='$FAM_WUI_BASE_HREF/'",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "postinstall": "patch-package --patch-dir node_modules/arlas-web-core/patches",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,fr}.json --format json",
    "license-check": "license-check-and-add check -f license-check-and-add.json"
  },
  "private": true,
  "peerDependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "rxjs": "^7.4.0",
    "zone.js": "^0.11.4"
  },
  "dependencies": {
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "arlas-wui-toolkit": "27.0.0-rc.1",
    "ngx-spinner": "15.0.1",
    "ngx-toastr": "~16.2.0",
    "patch-package": "^6.4.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@vendure/ngx-translate-extract": "8.3.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.57.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "license-check-and-add": "^4.0.5",
    "node": "^18.19.1",
    "typescript": "4.9.3"
  },
  "overrides": {
    "angular-oauth2-oidc-jwks": {
      "jsrsasign": "^11.0.0"
    },
    "braces": "3.0.3",
    "socks": "2.7.3",
    "@angular-devkit/build-angular": {
      "webpack-dev-middleware": "~5.3.4"
    },
    "http-proxy-middleware": "2.0.7",
    "shpjs": "4.0.4",
    "@mapbox/geojson-extent": {
      "traverse": "0.6.8"
    },
    "dompurify": "2.5.6",
    "mermaid": "10.9.3"
  }
